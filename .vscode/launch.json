{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter (Chrome)",
      "program": "lib/main.dart",
      "cwd": "ketodiet_flutter_project",
      "deviceId": "chrome",
      "request": "launch",
      "type": "dart",
      "args": ["--web-hostname", "127.0.0.1", "--web-port", "8000"]
    },
    {
      "name": "ketodiet_flutter_project",
      "cwd": "ketodiet_flutter_project",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "ketodiet_flutter_project (profile mode)",
      "cwd": "ketodiet_flutter_project",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "ketodiet_flutter_project (release mode)",
      "cwd": "ketodiet_flutter_project",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
